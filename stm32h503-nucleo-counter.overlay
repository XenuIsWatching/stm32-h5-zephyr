
#include <st/h5/stm32h503rbtx-pinctrl.dtsi>

/ {
        zephyr,user {
                capture-tester-gpios = <&gpioc 6 GPIO_ACTIVE_HIGH>,
                                       <&gpioc 7 GPIO_ACTIVE_HIGH>;
        };
};

&timers2 {
        st,prescaler = <239>;
        status = "okay";

        capture: counter {
                status = "okay";
                pinctrl-0 = <&tim2_ch1_pa0 &tim2_ch2_pa1>;
                pinctrl-names = "default";
        };
};
